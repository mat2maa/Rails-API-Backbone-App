<!--templates/index.jst.ejs-->

<table class="table">
  <thead>
    <tr>
      <td>ID</td>
      <td>API Key</td>
      <td>Username</td>
      <td>First Name</td>
      <td>Last Name</td>
      <td>Date of Birth</td>
    </tr>
  </thead>

  <tbody>
    <% _.each(users, function(user) { %>
      <tr>
        <td><%= user.get("id") %></td>
        <td><%= user.get("api_key") %></td>
        <td><%= user.get("username") %></td>
        <td><%= user.get("first_name") %></td>
        <td><%= user.get("last_name") %></td>
        <td><%= user.get("date_of_birth") %></td>
      </tr>
    <% }); %>
  </tbody>
</table>